 
{
  "schemaVersion": 1,
  "id": "elytraautopilot",
  "version": "${version}",

  "name": "Elytra AutoPilot",
  "description": "Automatic elytra flight with minimal use of fireworks",
  "authors": [
    "TheMegax",
    "LouisLOurson"
  ],
  "contact": {
    "homepage": "https://github.com/TheMegax/fabric-elytra-autopilot",
    "sources": "https://github.com/TheMegax/fabric-elytra-autopilot"
  },

  "license": "CC0-1.0",
  "icon": "assets/elytraautopilot/icon.png",

  "environment": "client",
  "entrypoints": {
    "client" : [
      "net.elytraautopilot.ElytraAutoPilot"
    ]
  },
  "mixins": [
    "elytraautopilot.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "flamingo": "*"
  }
}
